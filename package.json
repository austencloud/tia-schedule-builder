{
  "name": "museum-schedule-visualization",
  "version": "1.0.0",
  "description": "Museum staffing schedule visualization with Svelte 5 and glassmorphism design",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "magic": "node scripts/magical-dev.js",
    "magic:browser": "node scripts/magical-dev.js --browser",
    "magic:clean": "npm run clean && node scripts/magical-dev.js",
    "magic:reset": "npm run clean:deep && node scripts/magical-dev.js --browser",
    "f5": "npm run magic:browser",
    "clean": "rimraf node_modules/.vite && rimraf dist",
    "clean:deep": "rimraf node_modules && npm install"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "rimraf": "^6.0.1",
    "svelte": "^5.0.0",
    "vite": "^5.0.0"
  }
}
